      "bundleIdentifier": "coding.test.com"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "coding.test.com"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
